<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.example.gilbert">


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Attractions"
            android:label="@string/category_attractions" />
        <activity
            android:name=".BarsActivity"
            android:label="@string/category_bars" />
        <activity
            android:name=".HotelsActivity"
            android:label="@string/category_hotels" />
        <activity
            android:name=".EatsActivity"
            android:label="@string/category_eats" />
    </application>

</manifest>